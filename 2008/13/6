var host = "games.espn.go.com";
var gameRoot = "ffl";var leagueId = 97031;var teamId = 8;
var GAMEROOT = "ffl";



var xmlLink = "format/playerTableXML";
var popUpItr = 0;

function playerTable(context, view) {

	var scoringPeriodId = 13;
	
	var paramArr = Array(Array("parentContext", context), Array("scoringPeriodId", scoringPeriodId));
	paramStr = getURLParam(paramArr);
	
	
	var _complete = function(t) {
		loadPopUp();
	}
	
	var url = "http://" + host + "/" + gameRoot + "/" + xmlLink + paramStr + noCache();
	if (context == "rosterfix") url += "&" + view;
	new Ajax.Updater('pagedata', url, {method:'post', onComplete: _complete, evalScripts: true});

}
	
var time0 = 0;

function players(navParams) {
	
	var spinnerEl = $("playerTableTabSpinner");
	if (spinnerEl) spinnerEl.show();
	var realtimeScoringDiv = $('realtimeScoringTools');
	
	var version = null;
	var params = navParams.split("&");
	var parentContext = null;
	var view = null;
	var scoringPeriodId = null;
	for (var i = 0; i < params.length; i++) {
		var paramArr = params[i].split("=");
		if (paramArr.length > 1) {
			if (paramArr[1].indexOf("yourPlayers") > -1) {
				version = "yourPlayers";
			} else if  (paramArr[1].indexOf("incomingPlayers") > -1) {
				version = "incomingPlayers";
			}
		}
		if (paramArr[0] == "parentContext") parentContext = paramArr[1];
		else if (paramArr[0] == "view") view = paramArr[1];
		else if (paramArr[0] == "scoringPeriodId") {
			scoringPeriodId = parseInt(paramArr[1]);
			if (realtimeScoringDiv) {
				parseInt(paramArr[1]) == 18 ? realtimeScoringDiv.show() : realtimeScoringDiv.hide();
			}
		}
	}
	
	var _complete = function(t) {
			
		loadPopUp();
		
		
		/* here would would be a good place for omniture code */
		
		
		
		if (spinnerEl) spinnerEl.hide();
		if (chooser) { chooser.isOpen = false; chooser.getObjs(); }
	}
	var url = "http://" + host + "/" + gameRoot + "/" + xmlLink + "?" + navParams + noCache();

	var target = "pagedata";
	if (version != null) target += "_" + version;
	
	time0 = new Date().getTime();
	var confirmed = true;
	
		
	if (confirmed) new Ajax.Updater(target, url, {method:'post', onComplete: _complete, evalScripts: false});
	else if (spinnerEl) spinnerEl.hide();
	
	
}

function loadPopUp() {

	Behaviour.apply();
	
}

function noCache() {
	return "&r=" + Math.round(Math.random() * 99999999);
}



function getURLParam(newParams){
	
	var strReturn = "";
	var strHref = window.location.href;
	
	// REMOVE HASH
	hashIndex = strHref.indexOf("#");
	if (hashIndex > -1) {
		strHref = strHref.substr(0, hashIndex);
	}
	
	
	if (strHref.indexOf("?") > -1 ){
		
		var strQueryString = strHref.substr(strHref.indexOf("?"));
		var aQueryString = strQueryString.split("&");
		
		// LOOP THROUGH CURRENT PARAMS
		for (var i = 0; i < aQueryString.length; i++ ){
		
			var match = false;
			
			// LOOP THROUGH PARAMS THAT NEED TO BE REPLACED OR ADDED
			for(var j = 0; j < newParams.length; j++) {
				
				// PARAM MATCH?  IF SO, MARK
				if (aQueryString[i].indexOf(newParams[j][0] + "=") > -1 ){
					match = true;
					break;
				}
				
			}
			
			// IF NO MATCH, SAVE PARAM.  IF MATCH, SKIP
			if (!match) {
				strReturn += aQueryString[i] + "&";
			}
			
		}
		
	}
	
	// ADD IN NEW PARAMS TO END
	var amp = "";
	for (var j = 0; j < newParams.length; j++) {
		//alert(newParams[j][0] + " = " + newParams[j][1]);
		strReturn += amp + newParams[j][0] + "=" + newParams[j][1];
		amp = "&";
	}
	
	
	//alert(strReturn);
	return strReturn;
	
}


