function hideErrorMessageDiv () {

}


function findPos(obj) {
	var curleft = curtop = 0;
	if (obj.offsetParent) {
		curleft = obj.offsetLeft
		curtop = obj.offsetTop
		while (obj = obj.offsetParent) {
			curleft += obj.offsetLeft
			curtop += obj.offsetTop
		}
	}
	return [curleft,curtop];
}

StatChooser = Class.create();

StatChooser.prototype = {

	isOpen: false,
	statForm: null,
	chooserDiv: null,
	chooserLink: null,
	
	initialize: function() { },
	
	updateCookie: function(playerParams) {
		var formEls = this.statForm.elements;

		var pipe = "";
		var statIdsToShow = "";
		var thisEl;

		for (var i=0; i < formEls.length; i++) {
			thisEl = formEls[i];
			if (thisEl.name == "chooseLeagueStatsToDisplayChoice" && thisEl.type == "checkbox") {
				if (thisEl.checked) {
					statIdsToShow += pipe + thisEl.value;
					pipe = "|";
				}
			}
		}

		var success = function(t){
			players(playerParams);
		}
		var failure = function(t){ }

		//GET LEAGUEID FROM PARAMS
		var params = playerParams.split("&");
		var leagueId = -1;
		for (var i = 0; i < params.length; i++) {
			var paramArr = params[i].split("=");
			if (paramArr[0] == "leagueId") {
				leagueId = paramArr[1];
				break;
			}
		}	
		
		var url = "http://" + host + "/" + gameRoot + "/" + "util/saveLeagueStatsToDisplayToCookie?leagueId=" + leagueId + "&show=" + statIdsToShow + noCache();
		var myAjax = new Ajax.Request(url, {method:'get', onSuccess:success, onFailure:failure}); 
	},
	
	clearCookie: function(playerParams) {
		var success = function(t){ players(playerParams); }
		var failure = function(t){ }

		//GET LEAGUEID FROM PARAMS
		var params = playerParams.split("&");
		var leagueId = -1;
		for (var i = 0; i < params.length; i++) {
			var paramArr = params[i].split("=");
			if (paramArr[0] == "leagueId") {
				leagueId = paramArr[1];
				break;
			}
		}

		var url = "http://" + host + "/" + gameRoot + "/" + "util/clearLeagueStatsToDisplayCookie?leagueId=" + leagueId + noCache();
		var myAjax = new Ajax.Request(url, {method:'get', onSuccess:success, onFailure:failure});
	},
	
	getObjs: function() {
		this.statForm = $('chooseLeagueStatsToDisplayForm');
		this.chooserDiv = $('chooseLeagueStatsToDisplayDiv');
		this.chooserLink = $('chooseLeagueStatsToDisplayLink');
	},
	
	toggle: function() {
		if (!this.statForm || !this.chooserDiv || !this.chooserLink) this.getObjs();
			
		if (this.isOpen) this.chooserDiv.hide();
		else { 
			var location = findPos(this.chooserLink);
			this.chooserDiv.style.left = (location[0]-115)+"px";
			this.chooserDiv.style.top = (location[1]+13)+"px";
			this.chooserDiv.show();
		}
		this.isOpen = !this.isOpen;
	}

}

var chooser = new StatChooser();


Poll = Class.create();
Poll.barImagePath = 'http://games-ak.espn.go.com/s/ffllm/08/images/pollBar.gif';
Poll.theMonths = new Array("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec");
Poll.today = new Date();

Poll.prototype = {
	
	pollCode: -1,
	isTeamVoting: false,
	pollResponse: null,
	isPassive: true,
	isAllowedToVote: true,
	options: [],
	votes: [],
	maxBarWidth: 0,
	responseUrl: '',
	selectedOption: null,
		
	barWidth: 0,
	pollBodyElementName: '',
	pollOptionsElementName: '',
	pollSelOptionElementName: '',
	
	initialize: function(args) {	
	
		this.pollCode = args.pollCode;
		this.isTeamVoting = args.isTeamVoting;
		this.isPassive = args.isPassive;
		this.isAllowedToVote = args.isAllowedToVote;
		this.maxBarWidth = args.maxBarWidth;
		this.responseUrl = args.responseUrl;
		this.options = args.options;
		this.votes = args.votes;
		this.pollResponse = args.pollResponse;
		
		preloadImage = new Image(1, 10); 
      		preloadImage.src=Poll.barImagePath; 
		
		if (this.pollResponse != null && this.pollResponse.vote < 0) {
			this.pollResponse = null;
		}

		this.pollBodyElementName = 'poll' + this.pollCode + '_body';	
		this.pollOptionsElementName = 'poll' + this.pollCode + '_options';
		this.pollSelOptionElementName = 'poll' + this.pollCode + '_selOpt';	
		
		this.render();		
	},

	render:function() {
			
		if (this.pollResponse != null || !this.isAllowedToVote || this.isPassive) {

			this.renderResultsView();
		} else {		

			this.renderVoteView();			
		}
	},
	
	vote: function() {

		if (this.selectedOption > -1) {
		
			this.pollResponse = new PollResponse(this.selectedOption);
			v = this.pollResponse.vote
			if (this.isAllowedToVote) {
				
				this.votes[this.selectedOption]++;
				this.render();
				window.location.href = this.responseUrl + v;
			}
		} 
		
	},
	
	renderResultsView: function() {
		var newHtml = '';
		var totalVotes = 0;
		
		for(i = 0; i < this.votes.length; i++) {
			totalVotes += this.votes[i];
		}
		
		Element.update(this.pollBodyElementName, '');
	
		for(var ct = 0; ct < this.options.length; ct++) {
			var opt = this.options[ct];
			if (this.votes[ct] == 0) {
				var barWidth = 1;
				var votePct = 0;
			} else {
				var barWidth = Math.max(Math.round(this.maxBarWidth * (this.votes[ct] / totalVotes)), 1);
				var votePct = Math.round((100 / totalVotes) * this.votes[ct]);
			}
			var plural = (this.votes[ct] != 1 ? 's' : '');
			newHtml += '<div style="display:block;margin-top:8px;">';
			if (this.votes[ct] > 0) {
				newHtml += '<img src="' + Poll.barImagePath + '" height="10" width="' + barWidth + '" border="0" /> ';
			}
			newHtml += votePct + '%<br />' + opt;
			if (this.pollResponse != null && this.pollResponse.vote == ct) {
				newHtml += '*';
			}
			newHtml += ' <nobr>(' + this.votes[ct] + ' vote' + plural + ')</nobr></div>';
		}
		
		newHtml += '<div style="display:block;margin-top:16px;">'
		if (this.pollResponse != null) {
			newHtml += '<em>You';			
			//if (userResponse != null and user.userProfile.userProfileId != userResponse.userProfileId) { 
			//	newHtml += 'r team' 
			//} 			
			newHtml += ' voted on ' + this.pollResponse.voteDate + '</em><br>';
		} 
		
		newHtml += 'Total Votes: ' + totalVotes + '&nbsp;&nbsp;(One per ';
		if(this.isTeamVoting) { 
			newHtml += 'Team' 
		} else { 
			newHtml += 'Owner' 
		}; 
		newHtml += ')</div>';	
		new Insertion.Bottom($(this.pollBodyElementName), newHtml);
		//Element.update(this.pollBodyElementName, newHtml);
	},
	
	renderVoteView: function() {
		var _this = this;
		Element.update(this.pollBodyElementName, '');
		
		for(var ct = 0; ct < this.options.length; ct++) {
			radioId = this.pollOptionsElementName + ct;
		
			newHtml = '<input name="' + this.pollOptionsElementName + '" id="' + radioId + '" type="radio" value="' + ct + '" >' + this.options[ct] + '<br />\n';	
			new Insertion.Bottom($(this.pollBodyElementName), newHtml);		
			
			$(radioId).onclick = function () {
				_this.selectedOption = this.value;
			}
		}
		new Insertion.Bottom($(this.pollBodyElementName), '<br />');
		
		var voteButton = document.createElement('INPUT');
		voteButton.value = 'Vote!';
		voteButton.type = 'button';
		voteButton.className = 'sidebar';
		voteButton.onclick = function (event) {   
			_this.vote();
		};		
		$(this.pollBodyElementName).appendChild(voteButton);
			
		newHtml = '<br />(One vote per ';		
		if(this.isTeamVoting) { newHtml += 'Team' } else { newHtml += 'Owner' }; 
		newHtml += ')';
		
		new Insertion.Bottom($(this.pollBodyElementName), newHtml);	
	}
}


PollResponse = Class.create();
PollResponse.prototype = {

	vote: -1,
	voteDate: Poll.theMonths[Poll.today.getMonth()] + ' ' + Poll.today.getDate(),
	
	initialize: function(vote, voteDate) {
		this.vote = vote;
		if (voteDate == null) {
			this.voteDate = Poll.theMonths[Poll.today.getMonth()] + ' ' + Poll.today.getDate();
		} else {
			this.voteDate = voteDate;
		}
	}

}


function toggleBench(action, tmCount) {
	for (n=0;n<tmCount;n++) {
		if (action == 'show') {
			hide('benchShowLink' + n);
			show('bench' + n);
		} else {
			hide('bench' + n);
			show('benchShowLink' + n);
		}
	}
}</script>
